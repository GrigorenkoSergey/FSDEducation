mixin range-slider(obj = {})
  - 
    let {
      title = "range slider",
      leftRange = "5 000",
      rightRange = "10 000",
      currency = "â‚½",
      } = obj;
  .range-slider
    .range-slider__title #{title} 
    .range-slider__range #{leftRange}#{currency} - #{rightRange}#{currency}
    .range-slider__roller-container
      .range-slider__roller
        .range-slider__roller-left
        .range-slider__roller-right