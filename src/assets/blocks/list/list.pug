mixin list(options = {}) 
  -
    const defaultOptions = require('Blocks/list/default-options.json');
    const { title, list } = { ...defaultOptions, ...options};

  .list__title #{title}
  ul.list__ul
    each val in list
      if (val.link)
        li.list__li
          a.list__link(href= val.link) #{val.text}
      else
        li.list__li #{val.text}